{
  "name": "Chrome History Logger",
  "version": "0.1",
  "manifest_version": 4,
  "description": "Simple Chrome History Logger",
  "icons": {
    "16": "images/img16.png",
    "32": "images/img32.png",
    "48": "images/img48.png",
    "128": "images/img128.png"
  },
  "options_page": "src/options/index.html",
  "background": {
    "scripts": ["src/background/background.js"]
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "unlimitedStorage"
  ]
}
