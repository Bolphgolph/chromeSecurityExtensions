{
  "name": "Chrome Search Manipulation",
  "version": "0.1",
  "manifest_version": 4,
  "description": "Simple Chrome Search Manipulator",
  "homepage_url": "http://www.ericzhang.me/projects/chromelogger/",
  "icons": {
    "16": "images/img16.png",
    "32": "images/img32.png",
    "48": "images/img48.png",
    "128": "images/img128.png"
  },
  "options_page": "src/options/index.html",
  "background": {
    "scripts": ["src/inject/inject.js"]
  },
  "permissions": [
    "https://www.google.ch/*",
    "https://www.google.com/*",
    "tabs"
  ]
}
